  "data_type_mappings": {
    "nvarchar": "varchar",
    "nchar": "char", 
    "ntext": "text",
    "datetime": "timestamp",
    "datetime2": "timestamp",
    "smalldatetime": "timestamp",
    "bit": "boolean",
    "tinyint": "smallint",
    "money": "numeric(19,4)",
    "smallmoney": "numeric(10,4)",
    "uniqueidentifier": "uuid",
    "image": "bytea",
    "varbinary": "bytea",
    "binary": "bytea"
  },
  "performance_settings": {
    "postgresql": {
      "work_mem": "1GB",
      "maintenance_work_mem": "2GB", 
      "shared_buffers": "4GB",
      "effective_cache_size": "12GB",
      "checkpoint_completion_target": 0.9,
      "synchronous_commit": "off",
      "wal_buffers": "64MB",
      "max_parallel_workers_per_gather": 4
    },
    "bulk_operations": {
      "disable_triggers": true,
      "disable_indexes": false,
      "use_copy_command": true,
      "commit_batch_size": 50000,
      "parallel_loading": true
    }
  },
  "validation": {
    "row_count_comparison": true,
    "sample_data_verification": true,
    "foreign_key_integrity": true,
    "data_type_validation": true,
    "null_value_checking": true,
    "duplicate_detection": true,
    "custom_business_rules": []
  },
  "error_handling": {
    "continue_on_error": true,
    "max_errors_per_table": 100,
    "error_log_level": "WARNING",
    "create_error_tables": true,
    "quarantine_bad_data": true
  },
  "monitoring": {
    "progress_reporting_interval": 30,
    "performance_metrics": true,
    "memory_usage_tracking": true,
    "disk_space_monitoring": true,
    "connection_pool_monitoring": true
  },
  "security": {
    "encrypt_connection": true,
    "mask_sensitive_data": false,
    "audit_migration_activities": true,
    "data_classification_aware": false
  },
  "post_migration": {
    "update_statistics": true,
    "rebuild_indexes": true,
    "reset_sequences": true,
    "validate_constraints": true,
    "cleanup_staging_data": false,
    "generate_migration_report": true
  }
}